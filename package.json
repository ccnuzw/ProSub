{
  "name": "prosub",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:worker": "cd worker && wrangler dev --config wrangler.toml",
    "dev": "npm run build:frontend && npm-run-all -p dev:frontend dev:worker",
    "build:frontend": "node_modules/.bin/vite build",
    "build:worker": "wrangler deploy --config worker/wrangler.toml --minify --dry-run",
    "build": "npm run build:frontend && npm run build:worker",
    "deploy": "npm run build:frontend && cd worker && wrangler deploy",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/cookie": "^0.6.0",
    "@types/uuid": "^10.0.0",
    "ant-design-vue": "^4.2.6",
    "antd": "^5.26.5",
    "cookie": "^1.0.2",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "qrcode.vue": "^3.6.0",
    "uuid": "^11.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250718.0",
    "@eslint/eslintrc": "^3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.0.0",
    "eslint": "^9",
    "postcss": "^8.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.0.0",
    "typescript": "^5",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^5.0.0",
    "wrangler": "^4.25.0"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
