{
  "name": "prosub",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "wrangler pages dev --local --config wrangler.local.toml",
    "build": "vite build",
    "build:backend": "wrangler deploy",
    "clean": "rimraf dist dev-dist",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:local": "node scripts/test-local-db.js",
    "deploy": "npm run build && npm run build:backend",
    "init-db": "node scripts/init-db.js",
    "db:migrate": "wrangler d1 execute prosub-db --local --file=schema.sql",
    "db:migrate:prod": "wrangler d1 execute prosub-db --file=schema.sql"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@types/cookie": "^0.6.0",
    "@types/uuid": "^10.0.0",
    "ant-design-vue": "^4.2.6",
    "antd": "^5.26.5",
    "cookie": "^1.0.2",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "qrcode.vue": "^3.6.0",
    "uuid": "^11.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250718.0",
    "@eslint/eslintrc": "^3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.0.0",
    "esbuild": "^0.23.0",
    "eslint": "^9",
    "postcss": "^8.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.0.0",
    "typescript": "^5",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.0.2",
    "vue-tsc": "^2.0.0",
    "wrangler": "^4.25.0"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
